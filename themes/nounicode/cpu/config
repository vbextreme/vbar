#enable blink
blink = 1
#speed of blink in ms
blink.time = 450
#blink when cpu is over 90%
blink.on = 90
#refresh time in ms
refresh = 1000
#icon = "ðŸ’»"

#formatting output
#$@ icon 
#$0 all core
#$1 core 1
#$2 core 2
#$n etc etc

text.full = "cpu $0%"
text.short = "cpu $0%"

event = "${module.toggle(cpufreq,generic)}${modules.refresh()}"
